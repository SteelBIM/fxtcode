                                        <label class="col-sm-3  no-padding-right diy_fromlabel">停车状况</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_parkingstatus" class="datproject">
                                                <option value="0">-请选择-</option>
                                                @{
                                                    var parkingStatus = ViewBag.ParkingStatus as FxtDataAcquisition.NHibernate.Entities.LNKPAppendage;
                                                    int psCode = 0;
                                                    if (parkingStatus != null)
                                                    {
                                                        psCode = parkingStatus.ClassCode.Value;
                                                    }
                                                    foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.ParkingStatusCode)
                                                    {
                                                        <option value="@code.Code">@code.CodeName</option>
                                                    }
                                                }
                                            </select>
                                        </div>
                                    </div>
