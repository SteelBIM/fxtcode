<body xmlns="http://www.w3.org/1999/xhtml">
    <div class="page-content" style="padding-bottom:0px;">
        <div class="page-header">
            <h1>
                房号信息
            </h1>
        </div><!-- /.page-header -->

        <div class="row">
            <div class="col-xs-12" id="SubmitPanel">
                <form>
                    <input type="hidden" id="hdHouseId" name="id" value="@ViewBag.HouseId" />
                    <input type="hidden" id="hdBuildingId" name="BuildingId" value="@ViewBag.BuildingId" />
                    <input type="hidden" id="hdCityId" name="CityId" value="@ViewBag.CityId" />
                    <input type="hidden" id="UnitNo" name="UnitNo" />

                    <table id="sample-table-1" class="table table-striped table-bordered table-hover">
                        <thead style="display:none;">
                            <tr>
                                <th>Domain</th>
                                <th>Price</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">所在单元</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" value="@ViewBag.UnitNo" id="txt_un" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">所在室号<span style="color:red">*</span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" value="@ViewBag.HouseNo" id="txt_hn" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">起始楼层<span style="color:red">*</span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" name="FloorNo" value="@if(Model.HouseId == 0){}else{ @(Model.FloorNo)}" id="txt_floorno" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">终止楼层<span style="color:red">*</span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" name="EndFloorNo" value="@Model.EndFloorNo" id="txt_endfloorno" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">朝向</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_FrontCode" class="dathouse" name="FrontCode">
                                                <option value="0">-请选择-</option>
                                            </select>
                                            <input type="hidden" id="hdFrontCode" value="@Model.FrontCode" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">建筑面积</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" name="BuildArea" value="@(Model.BuildArea == null || Model.BuildArea == 0 ? "" : Model.BuildArea.ToString())" id="txt_buildarea" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">户型</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_HouseTypeCode" name="HouseTypeCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                            </select>
                                            <input type="hidden"  id="hdHouseTypeCode" value="@Model.HouseTypeCode" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">景观</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_SightCode" name="SightCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                            </select>
                                            <input type="hidden" id="hdSightCode" value="@Model.SightCode" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">噪音情况</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_noisecode" class="dathouse" name="NoiseCode">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.Noise)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.NoiseCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">用途</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_purposecode" class="dathouse" name="PurposeCode" onchange="SelectPurposeCode(this)">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.PurposeCode)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.PurposeCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">户型结构</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_structurecode" class="dathouse" name="StructureCode">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.StructureCode)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.StructureCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">通风采光</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_vdcode" class="dathouse" name="VDCode">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.VDCode)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.VDCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">备注</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <textarea style="width:600px" name="Remark" id="txt_remark" class="dathouse">@Model.Remark</textarea>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </form>
                @if (ViewBag.IsUpdateRight == 1)
                {
                    <div class="clearfix form-actions diy_form-actions">
                        <div class="col-md-offset-3 col-md-9 col-sm-3">
                            <input class="btn btn-info" type="button" value="保存" id="btnSubmit" />
                        </div>
                    </div>
                }
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</body>
</html>
