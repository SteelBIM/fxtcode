<body>
    <!--整块-->
    <div class="row" style="height:310px; float:left; width:595px;">
        <!--整块-->
        <div class="col-xs-12" style="height:100%;" id="SubmitPanel">
            <!--第一个表格-->
            <div class="row" id="userPanel" style="height:100%;">
                <div class="col-xs-13" style="text-align: left;">
                    <select id="selectDepartment">
                        <option value="0">小组</option>
                        @foreach (Department obj in ViewBag.DepartmentList)
                        {
                        <option value="@obj.DepartmentId">@obj.DepartmentName</option>
                        }
                    </select>
                    <input type="text" class="search-query" id="txtKeyword" placeholder="关键字" />
                    <button type="button" class="btn btn-inverse btn-xs diy_search_btn" id="btnSearch">
                        搜索
                        <i class="icon-search icon-on-right bigger-110"></i>
                    </button>
                </div>
                <div class="col-xs-12" id="tableWindow" style="height:220px; overflow-y:scroll;">
                    <div class="table-responsive" style=" overflow:hidden;">
                        <table id="sample-table-1" class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th class="center">
                                        &nbsp;
                                    </th>
                                    <th>用户名</th>
                                    <th>状态</th>
                                    <th>姓名</th>
                                    <th>
                                        <i class="icon-time bigger-110 hidden-480"></i>
                                        小组
                                    </th>
                                    <th class="hidden-480">角色</th>
                                </tr>
                            </thead>
                            <tbody id="userRowList">
                                <tr id="userRow" style="display:none;">
                                    <td class="center">
                                        <label>
                                            <input type="radio" name="selectUserName" class="ace cb_select" />
                                            <span class="lbl"></span>
                                        </label>
                                    </td>
                                    <td>
                                        <font class="txt_username fontshow" >admin@fxt</font>
                                    </td>
                                    <td class="txt_uservalid"></td>
                                    <td class="txt_truename"></td>
                                    <td class="txt_departmentname"></td>
                                    <td class="hidden-480">
                                        <span class="label label-sm label-warning txt_rolenames"></span>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div><!-- /.table-responsive -->
                </div><!-- /span -->
                <div class="col-xs-12">
                    <input type="hidden" value="" id="hdUserCount" />
                    <ul id="example" data-count=""></ul>
                </div>
                <div class="col-xs-12" style=" text-align: right;">
                    <input class="btn btn-info" type="button" value="提交" id="btnSubmit" />
                </div>
            </div>

        </div>
    </div>
    <input type="hidden" value="@ViewBag.NowDepartment" id="hdNowDepartment" />
    <input type="hidden" value="@ViewBag.ViewType" id="hdViewType" />
    <input type="hidden" value="@ViewBag.NowUserName" id="hdNowUserName" />
    <input type="hidden" value="@ViewBag.AllotIds" id="hdAllotIds" />
</body>
