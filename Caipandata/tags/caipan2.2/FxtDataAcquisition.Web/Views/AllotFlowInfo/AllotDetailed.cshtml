                                                <label>
                                                    说明<input type="text" value="@datCheck.CheckRemark2" class="txt_audit_remark txt_notaudit_remark" />
                                                    <small class="green">
                                                        <b>审核</b>
                                                    </small>
                                                    <button class="btn btn-xs btn-primary opSubmit" data-action="audit">通过</button><button class="btn btn-xs opSubmit" data-action="notaudit">不通过</button>
                                                    <span class="lbl"></span>
                                                </label>
                                            }
