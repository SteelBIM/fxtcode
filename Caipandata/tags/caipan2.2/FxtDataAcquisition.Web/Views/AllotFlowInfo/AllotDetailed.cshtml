                                                        <b>撤回到待查勘</b>
                                                    </small>
                                                    <button class="btn btn-xs btn-primary opSubmit" data-action="notsurvey">确定</button>
                                                    <span class="lbl"></span>
                                                </label>
                                            }
                                            @if (ViewBag.Status == ViewBag.Status2_1 && ViewBag.IsRight == 1)
