<body>
    <input type="hidden" id="hdDepartmentId" value="@ViewBag.DepartmentId" />
    <input type="hidden" id="hdResult" value="@ViewBag.Result" />
    <div class="page-content" style="padding-bottom:0px;">
        <div class="page-header">
            <h1>
                小组信息
            </h1>
        </div><!-- /.page-header -->
        <div class="alert alert-danger" style="display:none;" id="errorAlert_notexists">
            你修改的信息不存在或已被删除
        </div>
        <div class="row">
            <div class="col-xs-12" id="SubmitPanel">
                <!-- PAGE CONTENT BEGINS -->
                <form class="form-horizontal" role="form">
                    <div class="form-group ">
                        <div class="form-group diy_form-group2">
                            <label class="col-sm-3  no-padding-right diy_fromlabel"> 公司 </label>
                            <div class="col-sm-9 diy_fromdiv2">
                                <input type="text" disabled="disabled" value="@ViewBag.CompanyName" />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="form-group diy_form-group">
                            <label class="col-sm-3  no-padding-right diy_fromlabel"> 小组名 </label>
                            <div class="col-sm-9 diy_fromdiv">
                                <input type="text" id="txtDepartmentName" value="@ViewBag.DepartmentName" />
                            </div>
                        </div>
                    </div>
                    <div class="alert alert-danger diy_alert-danger" style="display:none;" id="errorAlert">
                        系统异常
                    </div>
                    <div class="clearfix form-actions diy_form-actions">
                        <div class="col-md-offset-3 col-md-9 col-sm-3" style="float:left;">
                            <input class="btn btn-info" type="button" value="提交" id="btnSubmit" />
                            <i class="icon-ok btn-lg" style="float:right; display:none;" id="alert_success">提交成功</i>
                        </div>
                    </div>

                </form>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
</body>
</html>

