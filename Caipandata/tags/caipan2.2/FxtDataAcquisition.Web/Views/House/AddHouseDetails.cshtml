<body xmlns="http://www.w3.org/1999/xhtml">
    <div class="page-content" style="padding-bottom:0px;">
        <div class="page-header">
            <h1>
                房号信息
            </h1>
        </div><!-- /.page-header -->

        <div class="row">
            <div class="col-xs-12" id="SubmitPanel">
                <form>
                    <input type="hidden" id="hdHouseId" name="Id" value="@Model.Id" />
                    <input type="hidden" id="hdBuildingId" name="BuildingId" value="@Model.BuildingId" />
                    <input type="hidden" id="hdCityId" name="CityID" value="@Model.CityID" />
                    <input type="hidden" id="hdFxtCompanyId" name="FxtCompanyId" value="@Model.FxtCompanyId" />
                    <table id="sample-table-1" class="table table-striped table-bordered table-hover">
                        <thead style="display:none;">
                            <tr>
                                <th>Domain</th>
                                <th>Price</th>
                                <th>Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">单元<span style="color:red"></span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" value="@Model.UnitNo" id="txt_un" name="UnitNo" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">物理层<span style="color:red">*</span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" value="@(Model.FloorNo==0?"":Model.FloorNo.ToString())" id="txt_FloorNo" name="FloorNo" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">实际层</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" value="@Model.NominalFloor" id="txt_NominalFloor" name="NominalFloor" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">室号<span style="color:red">*</span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" value="@Model.RoomNo" id="txt_RoomNo" name="RoomNo" class="dathouse" />
                                        </div>
                                    </div>
                                </td>

                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">朝向</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_FrontCode" name="FrontCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                            </select>
                                            <input type="hidden" id="hdFrontCode"  value="@(Convert.ToInt32(Model.FrontCode))" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">建筑面积</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" name="BuildArea" value="@(Model.BuildArea == null || Model.BuildArea == 0 ? "" : Model.BuildArea.ToString())" id="txt_BuildArea" class="dathouse" />
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">房号名称<span style="color:red">*</span></label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <input type="text" name="HouseName" value="@Model.HouseName" id="txt_HouseName" class="dathouse" />
                                        </div>
                                    </div>
                                </td>

                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">景观</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_SightCode" name="SightCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                            </select>
                                            <input type="hidden" id="hdSightCode" value="@(Convert.ToInt32(Model.SightCode))" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">噪音情况</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_NoiseCode" name="NoiseCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.Noise)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.NoiseCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">用途</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_PurposeCode" name="PurposeCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.PurposeCode)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.PurposeCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">户型结构</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_StructureCode" name="StructureCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.StructureCode)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.StructureCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">通风采光</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_VDCode" name="VDCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                                @foreach (FxtDataAcquisition.Domain.Models.SYSCode code in ViewBag.VDCode)
                                                {
                                                    <option value="@code.Code" @(code.Code == Model.VDCode ? " selected=\"selected\"" : "")=)>@code.CodeName</option>
                                                }
                                            </select>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">户型</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <select id="txt_HouseTypeCode" name="HouseTypeCode" class="dathouse">
                                                <option value="0">-请选择-</option>
                                            </select>
                                            <input type="hidden" id="hdHouseTypeCode"  value="@(Convert.ToInt32(Model.HouseTypeCode))" />
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <div class="form-group diy_form-group">
                                        <label class="col-sm-3  no-padding-right diy_fromlabel">备注</label>
                                        <div class="col-sm-9 diy_fromdiv">
                                            <textarea style="width:400px" id="txt_Remark" name="Remark" class="dathouse">@Model.Remark</textarea>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                    @if (ViewBag.IsUpdateRight == 1)
                    {
                        <div class="clearfix form-actions diy_form-actions">
                            <div class="col-md-offset-3 col-md-9 col-sm-3">
                                <input class="btn btn-info" type="button" value="保存" id="btnSubmit" />
                            </div>
                        </div>
                    }
                </form>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div>
    <input type="hidden" id="hdProvinceId" value="@ViewBag.ProvinceId" />
    <input type="hidden" id="hdIsUpdateRight" value="@ViewBag.IsUpdateRight" />

</body>
</html>
