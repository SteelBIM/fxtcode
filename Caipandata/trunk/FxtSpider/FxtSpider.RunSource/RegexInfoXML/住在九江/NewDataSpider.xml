  <regexinfo Name="time" Remark="字符格式中提取时间正则(独有正则)">
    <!--([^\s]+) ([^\s]+) ([^\s]+) ([^\s]+) ([^\s]+) ([^\s]+)-->
    <regex>(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)</regex>
    <index>$6-{0}-$3 $4</index>
  </regexinfo>
  <regexinfo Name="time2" Remark="字符格式中提取月份正则(独有正则)">
    <!--([^\s]+) ([^\s]+) ([^\s]+) ([^\s]+) ([^\s]+) ([^\s]+)-->
    <regex>(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)%20(%5b%5e%5cs%5d%2b)</regex>
    <index>$2</index>
  </regexinfo>
