@{
    ViewBag.Title = "抓取后台维护与管理_案例爬取管理_案例搜索";
    Layout = "~/Views/Shared/Admin.cshtml";
}
@section bootstraplink{
    <!--表格相关-->
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/uniform.css" />
    <!--表单相关-->
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/colorpicker.css" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/datepicker.css" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/uniform.css" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/bootstrap-wysihtml5.css" />
}
@section bootstrapscript{ 
    <!--表单相关-->   
    <script src="/bootstrap/matrix-admin00/js/bootstrap-colorpicker.js"></script> 
    <script src="/bootstrap/matrix-admin00/js/bootstrap-datepicker.js"></script> 
    @*ie8报错*@
    <script src="/bootstrap/matrix-admin00/js/masked.js"></script> 
     @*ie8报错*@
    <script src="/Scripts/Spider/CaseSearch-matrix.form_common.js" type="text/javascript"></script>
    <script src="/bootstrap/matrix-admin00/js/wysihtml5-0.3.0.js"></script> 
    <script src="/bootstrap/matrix-admin00/js/jquery.peity.min.js"></script> 
    <script src="/bootstrap/matrix-admin00/js/bootstrap-wysihtml5.js"></script>
}
@section link{
  <link type="text/css" rel="Stylesheet" href="/Css/Spider/CaseSearch.css" />
}
@section script{ 
    <script src="/Scripts/My97DatePicker/WdatePicker.js" type="text/javascript"></script>
    <script type="text/javascript" src="/Scripts/Spider/CaseSearch.js"></script>
    <script type="text/javascript">
        $(function () {
            SetLeftMenu("2_1");
        });
    </script>
}
<div class="container-fluid" style="height:85% !important; width:100%;">
    <div class="row-fluid _row-fluid" id="dataPanel">
        <div class="widget-box _widget-box2"  id="">
          <div class="widget-title"> 
            <div class="dataTables_length  dataTables_length2">
                <label>
                    城市: 
                    <select id="selectCity" class="selectcity">
                         <option value="0" selected="selected">--全部--</option>
                        @foreach (FxtSpider.DAL.LinqToSql.城市表 obj in ViewBag.CityList)
                        {
                         <option value="@obj.ID">@obj.城市名称</option>
                        }
                    </select>
                </label>
            </div>
            <div class="dataTables_length  dataTables_length2">
                <label>
                    网站: 
                    <select id="selectWeb" class="selectweb">
                         <option value="0" selected="selected">--全部--</option>
                        @foreach (FxtSpider.DAL.LinqToSql.网站表 obj in ViewBag.WebList)
                        {
                         <option value="@obj.ID">@obj.网站名称</option>
                        }
                    </select>
                </label>
            </div>
            <div class="control-group" >
              <label class="control-label" style="float:left; padding-top:4px;">案例时间: </label>
              <div class="controls" style="width:100px; float:left;">
                <input type="text" placeholder="开始时间" value="@DateTime.Now.AddDays(-7).ToString("yyyy-MM-dd")" id="selectStartDate" onfocus="WdatePicker()"  style="height:18px;width:80px"/> 
              </div>
              <label class="control-label" style="float:left; padding-top:4px;">到</label>
              <div class="controls" style="width:100px; float:left;">
                <input type="text" placeholder="结束时间" value="@DateTime.Now.AddDays(-1).ToString("yyyy-MM-dd")" id="selectEndDate" onfocus="WdatePicker()" style=" height:18px; width:80px"/>
              </div>
            </div>
            <div class="dataTables_length  dataTables_length3">
              <button class="btn icon-search btn-inverse" id="btnSearch">查询</button>
            </div>
            <div class="dataTables_length  dataTables_length4">
              <button class="btn icon-download-alt btn-inverse" id="btnExcel" style="display:none;">导出Excel</button>
            </div>
          </div>
          <div class="widget-content nopadding tablepanel" style=" height:90%;">
           <div style="width:5000px; height:100%;">
            <table class="table table-bordered data-table">
              <thead>
                <tr role="row" class="table2">
                  <th>来源.</th>
                  <th>城市*</th>
                  <th>行政区.</th>
                  <th>片区</th>
                  <th>楼盘名*</th>
                  <th>楼栋.</th>
                  <th>房号.</th>
                  <th>案例时间.</th>
                  <th>用途*</th>
                  <th>面积*</th>
                  <th>单价*</th>
                  <th>总价*</th>
                  <th>案例类型*</th>
                  <th>结构.</th>
                  <th>建筑类型.</th>
                  <th>总楼层.</th>
                  <th>所在层.</th>
                  <th>户型.</th>
                  <th>朝向.</th>
                  <th>币种.</th>
                  <th>建筑年代.</th>
                  <th>装修.</th>
                  <th>配套设施.</th>
                  <th>URL.</th>
                  <th>电话.</th>
                  <th>地址</th>
                  <th>创建时间</th>
                  <th>建筑形式</th>
                  <th>花园面积</th>
                  <th>厅结构</th>
                  <th>车位数量</th>
                  <th>地下室面积</th>
                  <th>备注</th>
                </tr>
              </thead>
              <tbody id="caseList" data="">
                <tr class="gradeX" id="caseRom" style="display:none">
                  <td class="laiyuan center">来源数据</td>
                  <td class="chengshi center">城市数据</td>
                  <td class="xingzhengqu center">行政区数据</td>
                  <td class="pianqu center">片区数据</td>
                  <td class="loupan center">楼盘数据</td>
                  <td class="loudong center">楼栋数据</td>
                  <td class="fanghao center">房号数据</td>
                  <td class="anlishijian center">案例时间数据</td>
                  <td class="yongtu center">用途数据</td>
                  <td class="mianji center">面积数据</td>
                  <td class="danjia center">单价数据</td>
                  <td class="zongjia center">总价数据</td>
                  <td class="anlileixing center">案例类型数据</td>
                  <td class="jiegou center">结构数据</td>
                  <td class="jianzhuleixing center">建筑类型数据</td>
                  <td class="zonglouceng center">总楼层数据</td>
                  <td class="suozailouceng center">所在楼层数据</td>
                  <td class="huxing center">户型数据</td>
                  <td class="chaoxiang center">朝向数据</td>
                  <td class="bizhong center">币种数据</td>
                  <td class="niandai center">建筑年代数据</td>
                  <td class="zhuangxiu center">装修数据</td>
                  <td class="peitao center">配套设置数据</td>
                  <td class="titleUrl center">URL数据</td>
                  <td class="dianhua center">电话数据</td>
                  <td class="dizhi center">地址数据</td>
                  <td class="shijian center">创建时间数据</td>
                  <td class="xingshi center">建筑形式数据</td>
                  <td class="huayuan center">花园面积数据</td>
                  <td class="tingjiegou center">厅结构数据</td>
                  <td class="chewei center">车位数量数据</td>
                  <td class="dixiashi center">地下室面积数据</td>
                  <td class="infoTitle center">备注数据</td>
                </tr>
              </tbody>
              
            </table>
            
            </div>
          </div>
          
        <div class="widget-box widget-box2 _widget-box" style=" height:7%;" >
            <div class="example_page" style="width:500px; float:left; height:100%;">
                <div id="example"></div>
            </div>
            <div class="right_count" id="caseCount"></div>
        </div>
          
        </div>
    </div>
</div>
