<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html>
<head>
    <title>CaseSpiderErrorList</title> <meta http-equiv="X-UA-Compatible" content="IE=edge">
        @*设置浏览器IE文本模式为最高版本*@
    </meta>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/bootstrap-responsive.min.css" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/matrix-style.css" />
    <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/matrix-media.css" />
     <link rel="stylesheet" href="/bootstrap/matrix-admin00/css/select2.css" />
    <link href="/bootstrap/matrix-admin00/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/bootstrap/matrix-admin00/css/fonts.googleapis.com_css_family_Open_Sans_400,700,800.css" rel="stylesheet"  type='text/css'>   
     @*fancybox弹出框效果start*@
    <link href="/Scripts/jquery.fancybox-1.3.4/fancybox/jquery.fancybox-1.3.4.css" type="text/css" rel="Stylesheet" />

    <link href="/Css/Spider/CaseSpiderErrorList.css" type="text/css" rel="Stylesheet" />

    @*fancybox弹出框效果end*@
    <script src="/bootstrap/matrix-admin00/js/jquery.min.js" type="text/javascript"></script>
    <script src="/bootstrap/matrix-admin00/js/jquery.ui.custom.js" type="text/javascript"></script>
    <script src="/bootstrap/matrix-admin00/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/bootstrap/matrix-admin00/js/jquery.uniform.js" type="text/javascript"></script>
    <script src="/bootstrap/matrix-admin00/js/select2.min.js" type="text/javascript"></script>
    <script src="/bootstrap/matrix-admin00/js/matrix.js" type="text/javascript"></script>    
    <script src="/bootstrap/bootstrap-paginator/bootstrap-paginator.js" type="text/javascript"></script> 
    @*fancybox弹出框效果----start*@
    <script src="/Scripts/jquery.fancybox-1.3.4/fancybox/jquery.fancybox-1.3.4.js" type="text/javascript"></script>
    @*fancybox动画效果(可选*@)
    <script src="/Scripts/jquery.fancybox-1.3.4/fancybox/jquery.easing-1.3.pack.js" type="text/javascript"></script>
    @*fancybox鼠标滚轮滚动效果(可选)*@
    <script src="/Scripts/jquery.fancybox-1.3.4/fancybox/jquery.mousewheel-3.0.4.pack.js" type="text/javascript"></script>
    @*fancybox弹出框效果----end*@
    @*loadShow效果start*@
    <script src="/Scripts/jquery.blockUI.1.33/jquery.blockUI-1.33.js" type="text/javascript"></script>
    @*loadShow效果end*@
    <script type="text/javascript" src="/Scripts/jquery.freezeheader-1.5.0/js/jquery.freezeheader.js"></script>
    <script src="/Scripts/jquery.extend.js" type="text/javascript"></script>
    <script src="/Scripts/Common/Common.js" type="text/javascript"></script>
    <script src="/Scripts/Spider/CaseSpiderErrorList.js" type="text/javascript"></script>
</head>
<body>
    <input type="hidden" value="@ViewBag.CityId" id="hdCityId" />
    <input type="hidden" value="@ViewBag.CityName" id="hdCityName" />
    <input type="hidden" value="@ViewBag.WebId" id="hdWebId" />
    <input type="hidden" value="@ViewBag.WebName" id="hdWebName" />
    <input type="hidden" value="@ViewBag.StartDate" id="hdStartDate" />
    <input type="hidden" value="@ViewBag.EndDate" id="hdEndDate" />
    <div class="row-fluid" style="height:95% !important;" id="dataPanel">
        <div class="widget-box" id="tableWindow" style="overflow:auto !important; height:80%;">
                <table class="table table-bordered data-table" id="tableError">
                    <thead >
                        <tr role="row" class="table2">
                            <th style="width:100px;">城市</th>
                            <th style="width:100px;">网站</th>
                            <th style="width:100px;">错误类型</th>
                            <th style="width:250px;">URL</th>
                            <th style="width:120px;">时间</th>
                            <th style="width:310px;">说明</th>
                        </tr>
                    </thead>
                    <tbody id="errorInfoList" >
                        <tr class="gradeX" id="errorInfo" style="display:none;" >
                            <td class="center city" style="width:100px;">城市_数据</td>
                            <td class="center web" style="width:100px;">网站_数据</td>
                            <td class="center errorCode" style="width:100px;">错误类型_数据</td>
                            <td class="center url" style="width:250px;">URL_数据</td>
                            <td class="center date" style="width:120px;">时间_数据</td>
                            <td class="center remark" style="width:310px;">说明_数据</td>
                        </tr>
                    </tbody>              
                </table>
        </div>
        <div class="widget-box widget-box2 _widget-box" style=" height:7%;" >
            <div class="example_page" style="width:500px; float:left;">
                <div id="example" data-count=""></div>
            </div>
        </div>
        <div class="widget-box  widget-box4 _widget-box" style=" height:12%;" >
            <span style="font-weight:bold;">统计:</span>
            | 总失败次数:<span id="txtErrorCount">@ViewBag.ErrorCount</span> |
            网络异常次数:<span id="txtNetWorkErrorCount">@ViewBag.NetWorkErrorCount</span> |
            规则错误次数:<span id="txtRegexErrorCount">@ViewBag.RegexErrorCount</span> |
            需输入验证码次数:<span id="txtVCodeErrorCount">@ViewBag.VCodeErrorCount</span> 
        </div>
    </div>
    <div style="display:none">
    <select id="selectErrorCode">
     @foreach (FxtSpider.DAL.LinqToSql.SYS_Code obj in ViewBag.ErrorCodeList)
     {
     <option class="code_@obj.Code" value="@obj.Code">@obj.CodeName</option>
     }
    </select>
    </div>
</body>
</html>
