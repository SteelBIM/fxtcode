        <div class="widget-box widget-box2 _widget-box" style=" height:7%;" >
            <div class="example_page" style="width:500px; float:left;">
                <div id="example" data-count=""></div>
            </div>
            @*<div  class="right_count">
                共条案例
            </div>*@
        </div>
        <div class="widget-box  widget-box4 _widget-box" style=" height:5%;" >
            <span style="font-weight:bold;">统计:</span>
            | 案例总数:<span id="txtCaseCount"></span> |
            用途:<span id="txtPurpose">@ViewBag.PurposeCodeName</span> |
            面积段:<span id="txtBuildingArea">@ViewBag.AreaTypeCodeName</span> |
            建筑类型:<span id="txtBuildingType">@ViewBag.BuildingTypeName</span> |
            计算日期:<span id="txtDate">@ViewBag.Date</span> |
            计算均价:<span id="txtAvgPrice">@ViewBag.AvgPrice</span> |
            涨跌幅:<span id="txtFloat">@ViewBag.FloatValue</span> |
        </div>
        <div class="widget-box  widget-box3 _widget-box" style=" height:17%;" >
            <span style="font-weight:bold;">操作:</span>
            <input type="button" value="插入案例" id="addCase" class="btn btn-mini btn-primary" />
            <a href="javascript:;" class="btn btn-danger btn-mini text" id="delCase">删除案例</a>
            <input type="button" value="重新计算均价" class="btn btn-mini up"  id="btnResetCross"  />
        </div>
        <div class="widget-box widget-box2 _widget-box" style="display:none;">
            用途:
            <select id="selectPurposeCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.PurposeCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            案例类型:
            <select id="selectCaseTypeCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.CaseTypeCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            结构:
            <select id="selectStructureCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.StructureCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            建筑类型:
            <select id="selectBuildingTypeCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.BuildingTypeCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            户型:
            <select id="selectHouseTypeCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.HouseTypeCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            朝向:
            <select id="selectFrontCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.FrontCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            币种:
            <select id="selectMoneyUnitCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.MoneyUnitCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            装修:
            <select id="selectFitmentCode">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_SYSCode obj in ViewBag.FitmentCodeList)
                {
                <option value="@obj.Code" class="code_@obj.Code">@obj.CodeName</option>
                }
            </select>
            楼盘:
            <select id="selectProject">
                <option value="0" selected="selected">--全部--</option>
                @foreach (FxtSpider.FxtApi.Model.FxtApi_DATProject obj in ViewBag.ProjectList)
                {
                <option value="@obj.ProjectId" class="project_@obj.ProjectId">@obj.ProjectName</option>
                }
            </select>
            行政区:
            <select id="selectArea">
                <option value="0" selected="selected">--全部--</option>
            </select>
        </div>
    </div>
    <input type="hidden" id="hdFxtCompanyIds" value="@ViewBag.FxtCompanyIds" /> 
