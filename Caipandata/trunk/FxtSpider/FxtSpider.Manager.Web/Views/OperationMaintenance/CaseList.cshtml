                <table class="table table-bordered data-table" id="tableCase">
                    <thead style="" >
                        <tr role="row" class="table2">
                            <th><input type="checkbox" value="" title="全选" class="cbSelectAll" /></th>
                            <th  style="width:90px;">操作</th>
                            <th>楼盘*</th>
                            <th>楼栋</th>
                            <th>房号</th>
                            <th>用途*</th>
                            <th>面积*</th>
                            <th>单价*</th>
                            <th>总价*</th>
                            <th>案例类型*</th>
                            <th>结构</th>
                            <th>建筑类型</th>
                            <th>所在层</th>
                            <th>总楼层</th>
                            <th>户型</th>
                            <th>朝向</th>
                            <th>币种</th>
                            <th>案例时间</th>
                            <th>备注</th>
                            <th>来源</th>
                            <th>URL</th>
                            <th>电话</th>
                            <th>行政区</th>
                            <th>建筑年代</th>
                            <th>装修</th>
                            <th>附属房屋</th>
                            <th>配套</th>
                            <th>录入人</th>
                        </tr>
                    </thead>
                    <tbody id="caseList" >
                        <tr class="gradeX" id="caseRom" style="display:none;" >
                            <td class="check center"><input type="checkbox" class="sel_check" class="text"  value="" /></td>
                            <td class="option center" style="width:90px;">
                                <input type="button" value="修改" class="btn btn-mini up" data-caseId=""  />
                                <a href="javascript:;" class="btn btn-danger btn-mini text del" data-caseId="">删除</a>
                            </td>
                            <td class="loupan center" data-projectId="">楼盘数据</td>
                            <td class="loudong center">楼栋数据</td>
                            <td class="fanghao center">房号数据</td>
                            <td class="yongtu center" data-code="">用途数据</td>
                            <td class="mianji center">面积数据</td>
                            <td class="danjia center">
                                <span class="_text">单价数据</span>
                                <span>
                                    <input type="text"  class="_text2" data-caseId=""/>
                                    <input type="button" value="修改" class="btn btn-mini updanjia" data-caseId=""  />
                                    <input type="button" value="确定" class="btn btn-mini btn-primary subdanjia" data-caseId=""  />
                                    <input type="button" value="取消" class="btn btn-mini canceldanjia" data-caseId=""  />
                                </span>
                            </td>
                            <td class="zongjia center">总价数据</td>
                            <td class="anlileixing center" data-code="">案例类型数据</td>
                            <td class="jiegou center" data-code="">结构数据</td>
                            <td class="jianzhuleixing center" data-code="">建筑类型数据</td>
                            <td class="suozailouceng center">所在楼层数据</td>
                            <td class="zonglouceng center">总楼层数据</td>
                            <td class="huxing center" data-code="">户型数据</td>
                            <td class="chaoxiang center" data-code="">朝向数据</td>
                            <td class="bizhong center" data-code="">币种数据</td>
                            <td class="anlishijian center">案例时间数据</td>
                            <td class="infoTitle center">备注数据</td>
                            <td class="laiyuan center">来源数据</td>
                            <td class="titleUrl center">URL数据</td>
                            <td class="dianhua center">电话数据</td>
                            <td class="xingzhengqu center" data-areaId="">行政区数据</td>
                            <td class="niandai center">建筑年代数据</td>
                            <td class="zhuangxiu center" data-code="">装修数据</td>
                            <td class="fushu center">附属房屋数据</td>
                            <td class="peitao center">配套数据</td>
                            <td class="ruluren center">录入人数据</td>
                        </tr>
                    </tbody>              
                </table>
            </div>
