                                                    index3++;
                                                    if (index3 == 3 || (index3 == index2 && index4 == index))
                                                    {
                                                        index4++;
                                                        index3 = 0;
                                                        @Html.Raw("</tr>")
                                                    }

                                                }
                                            }
                                        </table>
                                    </fieldset>
                                }
                            }
                        }
                    }
