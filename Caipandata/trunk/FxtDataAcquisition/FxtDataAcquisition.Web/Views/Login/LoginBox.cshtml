<body class="login-layout" style="overflow:hidden; ">
    <div id="login-box" class="login-box visible widget-box no-border" style="margin-top: 0px; height: 100%;">
        <div class="widget-body" id="navbar" style="height: 100%;">
            <div class="widget-main" id="loginPanel" style="height:100%;">
                <h4 class="header blue lighter bigger" id="titleUserInfo">
                    <i class="icon-coffee green"></i>
                    请输入您的信息
                </h4>
                <h4 class="header blue lighter bigger" id="titleCityInfo" style="display:none;">
                    <i class="icon-coffee green"></i>
                    请选择所在城市
                </h4>

                <div class="space-6"></div>

                <form id="SubmitPanel">
                    <fieldset>
                        <label class="block clearfix">
                            <span class="block input-icon input-icon-right">
                                <input type="text" class="form-control" id="txtUserName" value="@ViewBag.LastUserName" placeholder="用户名" />
                                <i class="icon-user"></i>
                            </span>
                        </label>

                        <label class="block clearfix">
                            <span class="block input-icon input-icon-right">
                                <input type="password" class="form-control" id="txtPwd" placeholder="密码" />
                                <i class="icon-lock"></i>
                            </span>
                        </label>

                        <div class="space"></div>

                        <div class="clearfix">
                            @*<label class="inline" style="display:none;">
                            <input type="checkbox" class="ace" />
                            <span class="lbl">记住我</span>
                        </label>*@

                            <button type="button" id="btnSubmit" class="width-35 pull-right btn btn-sm btn-primary">
                                <i class="icon-key"></i>
                                登录
                            </button>
                        </div>

                        <div class="space-4"></div>
                    </fieldset>
                </form>
            </div><!-- /widget-main -->

        </div><!-- /widget-body -->
    </div><!-- /login-box -->
    <input type="hidden" id="hdOpenType" value="box" />
</body>
</html>
