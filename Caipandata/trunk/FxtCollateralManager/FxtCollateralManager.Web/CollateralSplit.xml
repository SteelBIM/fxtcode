<?xml version="1.0" encoding="utf-8" ?>
<Content>
  <PCA>
    <Province N1="">(?:(?!省).)*省</Province>
    <City N1="">(?:(?!市).)*市</City>
    <Area N1="">(?:(?!((县)|(区))).)*((县)|(区))</Area>
  </PCA>
  <Address N1="*$">    
    <Name N1="((?:(?!路).)+路)">((?:(?!路).)+路(?:(?!号).)+号)</Name>
    <Name N1="((?:(?!街).)+街)">((?:(?!街).)+街(?:(?!号).)+号)</Name>
    <Name N1="((?:(?!道).)+道)">((?:(?!道).)+道(?:(?!号).)+号)</Name>
    <Name N1="((?:(?!街道).)+街道)">((?:(?!街道).)+街道(?:(?!号).)+号)</Name>
    <Name>((?:(?!以北).)+以北)</Name>
    <Name>((?:(?!以东).)+以东)</Name>
    <Name>((?:(?!以南).)+以南)</Name>
    <Name>((?:(?!以西).)+以西)</Name>
    <Name>((?:(?!路北).)+路北)</Name>
    <Name>((?:(?!路东).)+路东)</Name>
    <Name>((?:(?!路南).)+路南)</Name>
    <Name>((?:(?!路西).)+路西)</Name>
    <Name>((?:(?!交口).)+交口)</Name>
    <Name>((?:(?!街道).)+街道)</Name>
    <Name>((?:(?!道北).)+道北)</Name>
    <Name>((?:(?!道东).)+道东)</Name>
    <Name>((?:(?!道南).)+道南)</Name>
    <Name>((?:(?!道西).)+道西)</Name>
    <Name>((?:(?!大道).)+大道)</Name>
    <Name>((?:(?!侧).)+侧)</Name>
    <Name>((?:(?!路).)+路)</Name>
    <Name>((?:(?!道).)+道)</Name>
    <Name>((?:(?!号).)+号)</Name>    
    <Name>((?:(?!#).)+#)</Name>
  </Address>
  <Project N1="*$">
    <Name>((?:(?!小区).)+小区)</Name>
    <Name>((?:(?!大厦).)+大厦)</Name>
    <Name>((?:(?!公寓).)+公寓)</Name>
    <Name>((?:(?!花园小区).)+花园小区)</Name>
    <Name>((?:(?!花园).)+花园)</Name>
    <Name>((?:(?!花苑).)+花苑)</Name>
    <Name>((?:(?!苑).)+苑)</Name>
    <Name>((?:(?!广场).)+广场)</Name>
    <Name>((?:(?!新村).)+新村)</Name>
    <Name>((?:(?!家园).)+家园)</Name>
    <Name>((?:(?!嘉园).)+嘉园)</Name>
    <Name>((?:(?!村).)+村)</Name>    
  </Project>
  <PInstallments N1="*$">
    <Name>((?:(?!期).)+期)</Name>
    <Name>((?:(?!区).)+区)</Name>
    <Name>((?:(?!组团).)+组团)</Name>
    <Name>((?:(?!片区).)+片区)</Name>
    <Name>((?:(?!地块).)+地块)</Name>
  </PInstallments>  
  <Building N1="*$">
    <Name>((?:(?!栋).)+栋)</Name>
    <Name>((?:(?!幢).)+幢)</Name>
    <Name>((?:(?!号楼).)+号楼)</Name>
    <Name>((?:(?!座).)+座)</Name>
    <Name>((?:(?!#).)+#)</Name>
    <Name>((?:(?!幢楼).)+幢楼)</Name>
    <Name>((?:(?!号).)+号)</Name>
    <Name>((?:(?!单元).)+单元)</Name>
  </Building>
  <Floor N1="*$" Building="([0-9]{1,}层)|([0-9]{1,}.[0-9]{1,}层)">
    <Name>((?:(?!层).)+层)</Name>
    <Name>((?:(?!楼).)+楼)</Name>
  </Floor>  
  <HouseNo N1="*$" Floor="([0-9]{1,}跃[0-9]{1,}层)|([0-9]{1,}跃[0-9]{1,})|([0-9]{1,}跃)|([0-9]{1,})">
    <Name>((?:(?!号).)+号)</Name>
    <Name>((?:(?!#).)+#)</Name>
    <Name>((?:(?!门面).)+门面)</Name>
  </HouseNo>
</Content>