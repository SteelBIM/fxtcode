<div>
    具体楼盘：@Html.TextBox("ProjectName", null, new { @class = "m-wrap small" })
</div>
<div class="div">
    <div class="content_div">
        行政区：&nbsp;&nbsp;@Html.DropDownList("areaid", null, new { @class = "m-wrap small" })
    </div>
    <div class="content_div">
        片区：&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DropDownList("subareaid", null, new { @class = "m-wrap small" })
    </div>
    <div style="clear: both;"></div>
</div>
<div class="div">
    <div class="content_div">
        楼栋用途：@Html.DropDownList("PurposeCode", null, new { @class = "m-wrap small" })
    </div>
    <div class="content_div">
        建筑类型：@Html.DropDownList("buildingtypecode", null, new { @class = "m-wrap small" })
    </div>
    <div style="clear: both;"></div>
</div>
<div class="div">
    <div class="content_div">
        建筑结构：@Html.DropDownList("StructureCode", null, new { @class = "m-wrap small" })
    </div>
    <div class="content_div">
        建筑面积：<select class="td_input" id="TotalBuildAreaCompany_build" name="TotalBuildAreaCompany_build">
            <option value="1">大于等于</option>
            <option value="0">小于等于</option>
        </select><input type="text" value="" name="TotalBuildArea" id="TotalBuildArea" class="td_input" />㎡      
    </div>
    <div style="clear: both;"></div>
</div>
<div class="div">
    <div class="content_div">
        总楼层：&nbsp;&nbsp;<select class="td_input" id="TotalFloorCompany_build" name="TotalFloorCompany_build">
            <option value="1">大于等于</option>
            <option value="0">小于等于</option>
        </select><input type="text" value="" name="TotalFloor" id="TotalFloor" class="td_input" />
    </div>
    <div class="content_div">
        总户数：&nbsp;&nbsp;
        <select class="td_input" id="TotalNumberCompany_build" name="TotalNumberCompany_build">
            <option value="1">大于等于</option>
            <option value="0">小于等于</option>
        </select><input type="text" value="" name="TotalNumber" id="TotalNumber" class="td_input" />
    </div>
    <div style="clear: both;"></div>
</div>
<div class="div">
    <div class="content_div">
        建筑时间：<input type="text" value="" name="BuildDate" id="BuildDate" class="m-wrap small" />&nbsp;&nbsp;～&nbsp;&nbsp;<input type="text" value="" name="BuildDateTo" id="BuildDateTo" class="m-wrap small" />
    </div>
    <div class="content_div">
        创建日期：@Html.TextBox("BstarTime", null, new { @class = "m-wrap small" })&nbsp;&nbsp;～&nbsp;&nbsp;@Html.TextBox("BendTime", null, new { @class = "m-wrap small" })
    </div>
    <div style="clear: both;"></div>
</div>
<tr>
    <td colspan="2">
        <input type="checkbox" name="all_building" id="all_building" value="all_building"
            onclick="selectAll('building')" /><b>全选</b>
        <a style="cursor: pointer;" id="a_building" href="javascript:showOrhide('building');">收起∧</a>
    </td>
</tr>
