    <div class="content_div">
        行政区：&nbsp;&nbsp;&nbsp;&nbsp;@Html.DropDownList("houseareaid", null, new { @class = "m-wrap small" })
    </div>
    <div class="content_div">
        片区：&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;@Html.DropDownList("housesubareaid", null, new { @class = "m-wrap small" })
