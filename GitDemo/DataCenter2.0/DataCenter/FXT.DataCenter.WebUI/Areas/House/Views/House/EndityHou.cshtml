            margin-left: 30px;
            width: 95%;
            font-size: 14px;
            font-weight: normal;
            line-height: 20px;
            font-family: "Microsoft Yahei", "微软雅黑", "宋体", "Simsun", "Open Sans";
        }

        .ui-multiselect
        {
            height: 35px;
        }

        input.hasDatepicker
        {
            width: 205px;
        }

        .xubox_layer
        {
            top: 5px!important;
        }

        .select2-container
        {
            width: 220px!important;
        }
    </style>
}
