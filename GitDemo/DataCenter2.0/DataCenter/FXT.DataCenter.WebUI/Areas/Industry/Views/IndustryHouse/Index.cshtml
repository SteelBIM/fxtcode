                            </div>
                        </div>
                        <div style="width: 60%; float: left;">
                            <div>
                                调整价格系数
                            </div>
                            <div>
                                楼盘均价:<input type="text" value="@Model.ProjectAvePrice" id="pAveragePrice" style="width: 70px;" readonly/>&nbsp;&nbsp;&nbsp;&nbsp;
                                楼栋均价:<input type="text" value="@Model.BuildingAvePrice" id="AveragePrice" style="width: 70px;" readonly/>&nbsp;&nbsp;&nbsp;&nbsp;
                                百分比:<input type="text" value="@Model.ProWeight" id="proWeight" style="width: 70px;" readonly/>%
                            </div>
                            <div>
                                <input type="button" title="系数说明" id="btnPriceDetail" value="系数说明" class="btn thickbox"
                                    alt="#TB_inline?height=300&width=490&inlineId=BuildPriceDetail" />
                                <input type="button" value="计算价格系数" class="btn" onclick="SetPriceWeight();" />
                            </div>
                        </div>
                        <div style="clear: both;">
                        </div>
                    </div>
                </div>
            </div>

            <!--是否可估-->
            <div class="tab-pane" id="IsEValue">
                <div class="portlet-body">
                    <table id="tab_IsEValue" class="tbsta" style="line-height: 40px;">
                        @{
    data.Remove("HouseAttr");
    data.Add("HouseAttr", "IsEValue");
    Html.RenderPartial("House", Model, data);
                        }
                    </table>
                    <div style="width: 100%; height: 100%;">
                        <div style="width: 30%; float: left;">
                            <div>
                                从
                                <select style="width: 100px;" name="FloorNoSet" id="selFloor_IsEValue">
                                </select>
                                到
                                <select style="width: 100px;" name="FloorNoSet" id="selFloorTo_IsEValue">
                                </select>
                                层
                            </div>
                            <div>
                                从
                                <select style="width: 100px;" name="UnitNoSet" id="selUnitNo_IsEValue">
                                </select>
                                到
                                <select style="width: 100px;" name="UnitNoSet" id="selUnitNoTo_IsEValue">
                                </select>
                                单元
                            </div>
                            <div>
                                可评估:<select class="m-wrap small" style="width: 60px!important;" id="sel_IsEValue">
                                    <option value="1">是</option>
                                    <option value="0" selected="selected">否</option>
                                </select>
                                <input type="button" class="btn" value="设置" onclick="HouseObj.CommonSetValue($('#selFloor_IsEValue').find('option:selected').val(),
    $('#selFloorTo_IsEValue').find('option:selected').val(), $('#selUnitNo_IsEValue').find('option:selected').val(),
    $('#selUnitNoTo_IsEValue').find('option:selected').val(), $('#sel_IsEValue').find('option:selected').text(), '#tab_IsEValue');" />
                            </div>
                        </div>
                        <div style="width: 50%; float: left;">
                        </div>
                        <div style="clear: both;">
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
    <div>
        <input type="hidden" name="PriceDetail" id="PriceDetail" value="@Model.PriceDetail" />
        <button id="btnSubmit" type="button" onclick="javascript:HouseObj.Submit(this);" class="btn">
            提交</button>
    </div>
}
<div id="BuildPriceDetail" style="display: none;">
    <textarea id="txtPriceDetail" name="txt" clos="100" rows="5" style="margin: 0px; width: 470px; height: 280px;"></textarea>
</div>
