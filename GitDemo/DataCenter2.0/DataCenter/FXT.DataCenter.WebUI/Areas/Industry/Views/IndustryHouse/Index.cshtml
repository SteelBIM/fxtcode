            overflow-y: hidden;
        }
    </style>
}
@section Javascript{
    <script src="@Url.Content("~/Areas/Industry/Scripts/IndustryHouse.js")"></script>
    <script type="text/javascript">
        //面包屑导航
        $("#navigation .breadcrumb li:eq(1)").remove();
        $("#navigation .breadcrumb li:eq(2)").remove();
        var html = "<li><span><a href=\"/Industry/IndustryProject/Index\">工业基础数据</a></span><i class=\"icon-angle-right\"></i></li>";
        html += " <li><span><a href=\"/Industry/IndustryBuilding/Index?projectId=" + "@Model.ProjectId" + "\">工业楼栋</a></span><i class=\"icon-angle-right\"></i></li>";
        html += "<li><span><a>工业房号</a></span></li>";
        $("ul.breadcrumb").append(html);

        var selID = '#HouseName';
        $(document).ready(function () {
            HouseObj.InitLoad("@Model.ProjectId", "@Model.BuildingId", "@Model.FxtCompanyId", "@Model.TotalFloor", "@Model.ProjectAvePrice", "@Model.BuildingAvePrice", "@Model.PriceDetail", "@Model.ProWeight");

            $('#myTab a').click(function (e) {
                e.preventDefault();
                $(this).tab('show');
                selID = $(this).attr("href");
                var louceng = $("#tab_HouseName").siblings("table").find("tbody tr").length;
                var loucengStr = "", danyuanStr = "", priceFloorNoStr = "";
                for (var i = 1; i <= louceng; i++) {
                    loucengStr += "<option value='" + i + "'>" + i + "</option>";
                    if (i == parseInt(louceng / 2) + 1) {
                        priceFloorNoStr += "<option value='" + i + "' selected='selected'>" + i + "</option>";
                    } else {
                        priceFloorNoStr += "<option value='" + i + "'>" + i + "</option>";
                    }
                }

                $("#tab_HouseName").siblings("table").find("thead tr th").each(function (t) {
                    if (t > 1) {
                        danyuanStr += "<option value='" + t + "'>" + $.trim($(this).text()) + "</option>";
                    }
                });
                $("select[name='FloorNoSet']").each(function () {
                    $(this).empty().append(loucengStr);
                });
                $("select[name='UnitNoSet']").each(function () {
                    $(this).empty().append(danyuanStr);
                });
                $("#PriceFloorNo").empty().append(priceFloorNoStr);
            });

            @*var active = '@Session["active"]';
            if (active == "") {
                $("#myTab a:first").tab('show');
            }*@
            $("#myTab a:first").tab('show');
            $("#myTab li").each(function () {
                var hre = $(this).children("a").attr("href");
                if (active == hre) {
                    $(this).addClass("active");
                    $(active).addClass("active");
                }
            });
            $("#btnPriceDetail").click(function () {
                var detailVal = $("#PriceDetail").val(), txtPriceDetail = $("#txtPriceDetail").val();
                $("#txtPriceDetail").val('').val(detailVal);
                $(this).tb_windowAddFooter({
                    sender1: "sender1", //第一个按钮的ID
                    name1: "确 定", //第一个按钮的名称
                    sender2: "sender2", //第二个按钮的ID
                    name2: "取 消", //第二个按钮的名称
                    sen1func: function () { //第一个按钮的功能函数
                        txtPriceDetail = $("#txtPriceDetail").val();
                        txtPriceDetail = txtPriceDetail.replace(/<[^>].*?>/g, '');//移除html标记
                        $("#PriceDetail").val('').val(txtPriceDetail);
                        HouseObj.PriceDetail = txtPriceDetail;
                        $.post("/House/House/EndtityPriceDetail", { buildId: "@Model.BuildingId", priceDetail: txtPriceDetail, fxtcompanyId: "@Model.FxtCompanyId" }, function (jsonmsg) {
                            //if (jsonmsg.flag)
                            //    alert("温馨提示:修改");
                        }, "json");
                        top.tb_remove();
                    }
                });
            });
        });
    </script>

}
