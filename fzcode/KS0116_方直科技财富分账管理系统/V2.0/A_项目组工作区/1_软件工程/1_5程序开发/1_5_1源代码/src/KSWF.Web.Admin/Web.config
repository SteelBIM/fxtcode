      </setting>
      <setting name="KSWF_WFM_BLL_PayService_FZPayService" serializeAs="String">
        <!--<value>http://pay.kingsun.cn/Service/FZPayService.asmx</value>-->
        <value>http://192.168.3.4:8088/Service/FZPayService.asmx</value>
