<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Kingsun.IBS.BLL.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <section name="RedisConfig" type="Kingsun.SynchronousStudy.Common.RedisConfigInfo, Kingsun.SynchronousStudy.Common" />
    <section name="SyncRedisConfig" type="Kingsun.SynchronousStudy.Common.RedisOtherConfigInfo, Kingsun.SynchronousStudy.Common" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <RedisConfig WriteServerList="192.168.3.12:6000" ReadServerList="192.168.3.12:6000" MaxWritePoolSize="300" MaxReadPoolSize="300" AutoStart="true" LocalCacheTime="180" RecordeLog="false">
  </RedisConfig>
  <SyncRedisConfig WriteServerList="192.168.3.12:6001" ReadServerList="192.168.3.12:6001" MaxWritePoolSize="300" MaxReadPoolSize="300" AutoStart="true" LocalCacheTime="180" RecordeLog="false">
  </SyncRedisConfig>
