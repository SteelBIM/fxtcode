  </RedisConfig>-->
  <connectionStrings>
    <!--总库-->
    <add name="kingsunconstr" connectionString="Data Source=192.168.3.129;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008;Connect Timeout=0;" providerName="System.Data.SqlClient" />
    <add name="kingsun_sz" connectionString="Data Source=192.168.3.54;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />
    <!--此配置不可更改-->
    <add name="kingsun_bj" connectionString="Data Source=192.168.3.54;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />
    <add name="kingsun_rjpep" connectionString="Data Source=192.168.3.54;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />
    <add name="kingsun_ot" connectionString="Data Source=192.168.3.54;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />
    <add name="kingsun_qpy" connectionString="Data Source=192.168.3.129;Initial Catalog=FZ_InterestDubbingGame;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />

    <!--UUMS配置连接-->
    <add name="kingsunUUMS" connectionString="Data Source=192.168.3.129;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />
    <add name="KingsunConnectionStr" connectionString="Data Source=192.168.3.129;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />

    <!--BaseDB总库-->
    <add name="KingsunBaseDBConnectionStr" connectionString="Data Source=192.168.3.129;Initial Catalog=FZ_SynchronousStudy;User ID=sa;Password=Kingsunsoft2008" providerName="System.Data.SqlClient" />
  </connectionStrings>

  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--分库-->
    <add key="kingsun_version" value="sz|bj|rjpep|ot" />
    <!--部署地址-->
    <add key="FS_SelfAddress" value="http://localhost:50868/" />
    <!--2018-01-16,同步订单到总库开关-->
    <add key="StartOrderInfoJob" value="1" />
    <!--2018-01-16,同步趣配音报名用户信息开关-->
    <add key="StartDubbingDataJob" value="1" />
    <!--2018-01-16,同步趣配音报名用户名称开关-->
    <add key="StartUserInfoRedisFromUMSJob" value="1" />
    <!--2018-01-16,刷新学生所在学校排行榜redis数据开关-->
    <add key="StartSchoolRankListJob" value="1" />
    <!--2018-01-16,//趣配音排行榜定时任务开关-->
    <add key="StartRankListJob" value="1" />

    <!--定时时长-->
    <add key="SyncDubbing" value="10" />
    <!--同步趣配音报名用户名称-->
    <add key="SyncUserInfoRedisFromUMS" value="10" />
    <!--刷新学生所在学校排行榜redis数据-->
    <add key="UpdateUserSchoolRankList" value="10"/>

    <!--MOD初始化开关-->
    <add key="StartMOD2IBSFirst" value="1"/>
    <!--初始化频率  年-->
    <add key="FirstCreateDataInteval" value="1"/>

    <!--MOD同步同步学redis数据-->
    <add key="MOD2IBSInterval" value="5"/>
    <!--MOD同步同步学开关-->
    <add key="StartMOD2IBS" value="1"/>

    <!--学习报告同步开关-->
    <add key="StartLearningReport" value="1"/>
    <!--同步学习报告频率-->
    <add key="LearningReportInterval" value="10"/>

    <!--学习报告初始化开关-->
    <add key="StartInitLearningReport" value="0"/>
    <!--学习报告初始化频率-->
    <add key="InitLearningReportInterval" value="1"/>

    <!--学习报告发布之后当天初始化开关-->
    <add key="StartTodayInitLearningReport" value="0"/>

    <!--刷新课本剧排行榜redis数据-->
    <add key="SynVideoRank" value="1"/>

    <!--初始化学生报告数据（1-4：深圳版，5-8：北京版，9-12：其他版）-->
    <add key="TypeName" value="4"/>
    <!--可配置初始化时间-->
    <add key="StudyReportInitTime" value="2017-12-20 16:00:00"/>



 
    <!--单元测试初始化节点  1-深圳 2-北京  3-OT 4-人教-->
    <!--跟ExamPaperTypeName匹配 SZ  BJ  OT  RJ-->
    <add key="AppAreaVersion" value="SZ"/>
    <add key="ExamPaperTypeName" value="1"/>
    <!--单元测试入库开关-->
    <add key="StartExampaper2DbModel" value="0"/>
    <!--单元测试入库频率-->
    <add key="Exampaper2DbModelInterval" value="5"/>
    <!--单元测试初始化数据-->
    <add key="StartInitExampaper2DbModel" value="0"/>
    <!--数据起始条数-->
    <add key="pageIndex" value="1"/>

    <!--数据结束条数-->
    <add key="pageEnd" value="500000"/>

    <!--是否删除redis表-->
    <add key="IsRemove" value="0"/>

    <!--2018-01-30 IBS修复正式姓名-->
    <add key="StartRepairIBS" value="1"/>
        <!--2018-02-01 开启IBS注册用户同步到MOD开关-->
    <add key="StartIBS2MODJob" value="1"/>
    <!--2018-02-01 IBS注册用户同步到MOD频率  分钟-->
    <add key="IBSRegisterUser2MODInteval" value="10"/>
    
    
    
  </appSettings>
  <applicationSettings>
    <Kingsun.IBS.BLL.Properties.Settings>
      <setting name="Kingsun_IBS_BLL_RelationService_RelationService"
          serializeAs="String">
        <value>http://183.47.42.221:8026/RelationService/RelationService.svc</value>
      </setting>
    </Kingsun.IBS.BLL.Properties.Settings>
  </applicationSettings>
  <system.web>

    <httpRuntime targetFramework="4.5" />

    <compilation targetFramework="4.5" />

    <pages>
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
      </namespaces>
    </pages>
    <customErrors mode="Off"/>
  </system.web>

  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />

    <handlers>
      <remove name="ExtensionlessUrlHandler-ISAPI-4.0_32bit" />
      <remove name="ExtensionlessUrlHandler-ISAPI-4.0_64bit" />
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <add name="ExtensionlessUrlHandler-ISAPI-4.0_32bit" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness32" responseBufferLimit="0" />
      <add name="ExtensionlessUrlHandler-ISAPI-4.0_64bit" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness64" responseBufferLimit="0" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="FZUUMS_Relation2Soap" />
        <binding name="FZUUMS_UserServiceSoap" />
        <binding name="ServiceSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://183.47.42.221:8026/UserService/FZUUMS_Relation2.asmx"
          binding="basicHttpBinding" bindingConfiguration="FZUUMS_Relation2Soap"
          contract="FZUUMS_Relation2.FZUUMS_Relation2Soap" name="FZUUMS_Relation2Soap" />
      <endpoint address="http://183.47.42.221:8026/UserService/FZUUMS_UserService.asmx"
          binding="basicHttpBinding" bindingConfiguration="FZUUMS_UserServiceSoap"
          contract="FZUUMS_UserService.FZUUMS_UserServiceSoap" name="FZUUMS_UserServiceSoap" />
      <endpoint address="http://metadata.kingsun.cn/Service/MetadataService.asmx"
          binding="basicHttpBinding" bindingConfiguration="ServiceSoap"
          contract="MetadataService.ServiceSoap" name="ServiceSoap" />
    </client>
  </system.serviceModel>

</configuration>
